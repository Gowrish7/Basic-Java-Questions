import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
		    int top_ten=sc.nextInt();
		    int eleven_hun=sc.nextInt();
		    int tot_prize= ((top_ten*10)+(eleven_hun*90));
		    System.out.println(tot_prize);
		}
	}
}
