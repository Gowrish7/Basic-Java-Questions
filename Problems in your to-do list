import java.util.*;
class codeChef{
    public static void main (String[] args) {
        /* code */
        Scanner scn = new Scanner(System.in);
        int m=scn.nextInt();
        while(m-->0){
            int N= scn.nextInt();
		    int A[] = new int[N];
		    
		   int count =0;
		    for (int j=0;j<N;j++){
		        A[j]=scn.nextInt();
		        if(A[j]>=1000){
		            count++;
		        }
		    }
		  	System.out.println(count);  
        }
    }
}
