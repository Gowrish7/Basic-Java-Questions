/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;
/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<n;i++){
		    int alice_amount=sc.nextInt();
		    int bob_amount=sc.nextInt();
		    int charlie_amount=sc.nextInt();
		    if(alice_amount>charlie_amount && alice_amount>bob_amount){
		        System.out.println("Alice");
		    }
		    else if(bob_amount>alice_amount && bob_amount>charlie_amount){
		        System.out.println("Bob");
		    }
		    else{
		        System.out.println("Charlie");
		    }
		}
	}
}
